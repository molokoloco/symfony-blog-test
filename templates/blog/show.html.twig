
{% extends 'base.html.twig' %}

{% block body %}

  <article>
      <h2>{{ article.title }} [{{ article.createdAt | date('d/m/Y') }} @ {{ article.createdAt | date('H:m') }}]</h2>
      <div class="content">
          <img src="{{ article.image }}" alt="">
          {{ article.content | raw }}
      </div>
  </article>

{% endblock %}